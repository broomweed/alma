map {+ 4} [1 2 3 4]

+ 4 5
+ 5 #"A" (* should fail *)
lower 5 (* should fail *)
lower #"w"

cons 5 [1 2 3 4]
cons "a" [1 2 3 4] (* should fail *)

outer { + } [1 2 3] [2 3 4 5]

outer { cons } "asdf" ["blep" "blop" "blemp"]

apply { + 4 } 3

apply { + } 4 5

print { [ {+} {*} {/} {-} ] }

print { [ 1 2 3 4 ] }

