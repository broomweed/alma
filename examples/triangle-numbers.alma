# Print the triangular numbers from 1 to 4999.

func trianglenum:
    dip [0]
    while* [> 0] [
        dup | dip [+]
        - 1
    ].

func main:
    1
    while* [< 5000] [
        print "triangular number #"
        print dup
        print " is "
        println trianglenum dup
        + 1
    ].
